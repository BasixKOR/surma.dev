---
layout: none
title: Sitemap
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>http://78762.de/</loc>
		<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
		<changefreq>daily</changefreq>
	</url>
	<url>
		<loc>http://78762.de/index.html</loc>
		<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
		<changefreq>daily</changefreq>
	</url>
	<url>
		<loc>http://78762.de/notes.html</loc>
		<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
		<changefreq>daily</changefreq>
	</url>
	<url>
		<loc>http://78762.de/impressum.html</loc>
		<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
		<changefreq>never</changefreq>
	</url>
	<url>
		<loc>http://78762.de/about.html</loc>
		<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
		<changefreq>monthly</changefreq>
	</url>
	{% for post in site.posts %}
		{% if post.categories contains "project" %}
		<url>
			<loc>http://78762.de{{ post.url }}</loc>
			<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
			<changefreq>daily</changefreq>
		</url>
		{% endif %}
	{% endfor %}
	{% for post in site.posts %}
		{% if post.categories contains "note" %}
		<url>
			<loc>http://78762.de{{ post.url }}</loc>
			<lastmod>{{site.time | date: "%Y-%M-%d"}}</lastmod>
			<changefreq>weekly</changefreq>
		</url>
		{% endif %}
	{% endfor %}
</urlset>
